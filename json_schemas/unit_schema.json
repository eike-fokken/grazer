    "required": ["value", "unit"],
    "additionalProperties": false,
    "properties": {
      "unit": {
        "type": "string",
        "oneOf": [
          {"pattern": "^(Y|Z|E|P|T|G|M|k|h|da|d|c|m|mu|\u00B5|\u03BC|n|p|f|a|z|y)?(g|t|oz|lb)$"},
          {"pattern": "^([0-9]*(\\.[0-9]+)? )(Y|Z|E|P|T|G|M|k|h|da|d|c|m|mu|\u00B5|\u03BC|n|p|f|a|z|y)?(g|t|oz|lb)$"}
      ]},
      "value": {"type": "number"}
    }
  }
}